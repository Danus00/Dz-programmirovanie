#include <iostream>

using namespace std;


int main() {
    int a;
    int b;
    int c;
    cout <<"Enter a:";
    cin >> a;
    cout <<"Enter b:";
    cin >> b;
    cout <<"Enter c:";
    cin >> c;

    cout << a << "*" << "2" << " = " << a*2 << endl;
    cout << b << " - " << "3" << " = " << b-3 << endl;
    cout << c << "*" << c << " = " << c*c << endl;
    return 0;

}
